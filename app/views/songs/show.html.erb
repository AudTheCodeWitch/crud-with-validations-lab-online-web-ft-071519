<h1><%= @song.title %></h1>
<h2><%= @song.artist_name %></h2>
<p><%= @song.release_year %></p>
<p><%= @song.genre %></p>
<%= button_to "Edit", { action: "edit", id: @song.id } %>
<%= link_to 'Delete', song_path(@song),
            method: :delete,
            data: { confirm: "Are you sure?"} %>